{
  "network": "livenet",
  "port": 80,
  "services": [
    "dashd",
    "insight-api",
    "insight-ui",
    "web"
  ],
  "servicesConfig": {
    "dashd": {
      "spawn": {
        "datadir": "./data",
        "exec": "/xagc/allgamescoind"
      }
    },
    "insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "insight-api": {
      "routePrefix": "api",
      "enableCache": true
    }
  }
}
